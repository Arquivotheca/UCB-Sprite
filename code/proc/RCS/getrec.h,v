@/*
 * getrec.h --
 *
 *	Include file for the trace record processing.
 */

#ifndef _GETREC_H
#define _GETREC_H


typedef struct traceFile {
    FILE *stream;		/* Stream to read from. */
    int traceRecCount;		/* Counter of records read. */
    int numRecs;		/* Counter of number of records. */
} traceFile;

#endif
